{
  "name": "@mscs/console",
  "description": "NodeJS console library",
  "author": "Jason Schilling <jschilling@media-service.com>",
  "version": "0.1.0-DEV",
  "repository": "git@github.com:media-service-dev/console.git",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode=production",
    "build:dev": "webpack --config webpack.config.js --mode=development",
    "build:watch": "webpack --config webpack.config.js --watch --mode=development",
    "lint": "eslint \"./src/**/*\" \"./__tests__/**/*\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "docs": "typedoc --listInvalidSymbolLinks src/"
  },
  "files": [
    "dist/",
    "docs/",
    "yarn.lock",
    "README.md",
    "LICENSE"
  ],
  "resolutions": {
    "minimist": "^1.2.2"
  },
  "devDependencies": {
    "@mscs/eslint-config": "^0.1.2",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.9.2",
    "@types/semver": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "jest": "^25.1.0",
    "jest-each": "^25.1.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typedoc": "^0.17.1",
    "typescript": "^3.8.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "semver": "^7.1.3"
  }
