  "files": [
    "dist/",
    "docs/",
    "yarn.lock",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@mscs/eslint-config": "^0.2.1",
    "@tsconfig/node12": "^1.0.7",
    "@types/jest": "^26.0.20",
