    "@types/semver": "^7.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.19.0",
    "jest": "^26.6.3",
    "jest-each": "^26.6.2",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.14",
    "typedoc": "^0.20.20",
    "typescript": "^4.1.3",
    "webpack": "^5.20.1",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "semver": "^7.3.4"
  }
